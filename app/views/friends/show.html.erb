<div class="jumbotron">
  <h3 class="display-4">
    <%= @friend.firstName %>
    <%= @friend.lastName %>
  </h3>
  <br>
  <p class="lead">
    <%= @friend.email %> |
    <%= @friend.phone %> |
    <%= @friend.twitter %>
  </p>
  <hr class="my-4">
  <br><br>
  <%= link_to 'Edit', edit_friend_path(@friend), class:"btn btn-secondary" %>
  <%= link_to 'Back', friends_path, class:"btn btn-dark" %>
</div>
